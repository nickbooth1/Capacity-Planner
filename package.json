{
  "name": "capacity-planner",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "build:gateway": "nx build api-gateway",
    "build:gateway:prod": "nx build api-gateway --configuration=production",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "prepare": "husky || true",
    "prisma:generate": "pnpm run prisma:generate:shared && pnpm run prisma:generate:entitlement && pnpm run prisma:generate:assets && pnpm run prisma:generate:work",
    "prisma:generate:shared": "cd packages/shared-kernel && npx prisma generate",
    "prisma:generate:entitlement": "cd packages/entitlement-service && npx prisma generate",
    "prisma:generate:assets": "cd packages/assets-module && npx prisma generate",
    "prisma:generate:work": "cd packages/work-module && npx prisma generate",
    "prisma:migrate:dev": "pnpm run db:init && pnpm run prisma:migrate:dev:all",
    "prisma:migrate:dev:all": "pnpm run prisma:migrate:dev:shared && pnpm run prisma:migrate:dev:entitlement && pnpm run prisma:migrate:dev:assets && pnpm run prisma:migrate:dev:work",
    "prisma:migrate:dev:shared": "cd packages/shared-kernel && npx prisma migrate dev",
    "prisma:migrate:dev:entitlement": "cd packages/entitlement-service && npx prisma migrate dev",
    "prisma:migrate:dev:assets": "cd packages/assets-module && npx prisma migrate dev",
    "prisma:migrate:dev:work": "cd packages/work-module && npx prisma migrate dev",
    "prisma:migrate:deploy": "pnpm run prisma:migrate:deploy:shared && pnpm run prisma:migrate:deploy:entitlement && pnpm run prisma:migrate:deploy:assets && pnpm run prisma:migrate:deploy:work",
    "prisma:migrate:deploy:shared": "cd packages/shared-kernel && npx prisma migrate deploy",
    "prisma:migrate:deploy:entitlement": "cd packages/entitlement-service && npx prisma migrate deploy",
    "prisma:migrate:deploy:assets": "cd packages/assets-module && npx prisma migrate deploy",
    "prisma:migrate:deploy:work": "cd packages/work-module && npx prisma migrate deploy",
    "db:init": "psql -U postgres -h localhost -f scripts/init-database.sql",
    "db:seed": "pnpm run db:seed:shared && pnpm run db:seed:entitlement && pnpm run db:seed:assets && pnpm run db:seed:work",
    "db:seed:shared": "cd packages/shared-kernel && npx prisma db seed",
    "db:seed:entitlement": "cd packages/entitlement-service && npx prisma db seed",
    "db:seed:assets": "cd packages/assets-module && npx prisma db seed",
    "db:seed:work": "cd packages/work-module && npx prisma db seed",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:reset": "docker compose down -v && docker compose up -d",
    "docker:db:up": "docker compose -f docker-compose.dev.yml up -d postgres",
    "docker:db:down": "docker compose -f docker-compose.dev.yml down",
    "docker:rebuild": "docker compose build --no-cache",
    "dev": "concurrently \"pnpm docker:db:up\" \"pnpm nx serve api-gateway\" \"pnpm nx dev web\"",
    "dev:docker": "docker compose up",
    "dev:api": "pnpm nx serve api-gateway",
    "dev:web": "pnpm nx dev web",
    "dev:setup": "pnpm install && pnpm docker:db:up && sleep 5 && pnpm prisma:generate && pnpm prisma:migrate:dev",
    "dev:start": "./scripts/dev.sh start",
    "dev:stop": "./scripts/dev.sh stop",
    "dev:restart": "./scripts/dev.sh restart",
    "dev:rebuild": "./scripts/dev.sh rebuild",
    "dev:status": "./scripts/dev.sh status",
    "dev:shell": "./scripts/dev.sh shell",
    "dev:reset": "./scripts/dev.sh reset",
    "ops:create-org": "tsx scripts/ops/cli.ts create-org",
    "ops:grant-module": "tsx scripts/ops/cli.ts grant-module",
    "ops:list-orgs": "tsx scripts/ops/cli.ts list-orgs",
    "ops:bulk-import": "tsx scripts/ops/cli.ts bulk-import",
    "ops:health-check": "tsx scripts/ops/cli.ts health-check",
    "ops:backup": "tsx scripts/ops/cli.ts backup",
    "ops:restore": "tsx scripts/ops/cli.ts restore",
    "test:unit": "node scripts/test-by-type.js unit",
    "test:integration": "node scripts/test-by-type.js integration",
    "test:all": "node scripts/test-by-type.js all",
    "test:assets": "nx test assets-module",
    "test:assets:watch": "nx test assets-module --watch",
    "test:assets:coverage": "nx test assets-module --coverage"
  },
  "private": true,
  "devDependencies": {
    "@nx/esbuild": "21.3.11",
